@model MvcCv.Models.Entitiy.TblYeteneklerim

@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Yetenek Ekleme Sayfası</h2>

<form method="post">
        
        @Html.Label("Yetenek")
        @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
        <br />
        @Html.Label("Derece %")
        @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })    
        <br />
    <button class="btn btn-success">Kaydet</button>
</form>

